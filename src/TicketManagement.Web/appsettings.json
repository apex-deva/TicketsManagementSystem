{
  "ConnectionStrings": {
    "TicketDb": "Server=.; Database=Ticket_Core; MultipleActiveResultSets=true; Trusted_Connection=True; TrustServerCertificate=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "EventLog": {
      "LogLevel": {
        "Default": "None"
      }
    }
  },
  "BackgroundServices": {
    "TicketAutoHandler": {
      "Enabled": true,
      "IntervalMinutes": 1
    }
  },
  "AllowedHosts": "*"
}
